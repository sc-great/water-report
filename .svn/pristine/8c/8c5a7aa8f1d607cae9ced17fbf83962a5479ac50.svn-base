<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org">
<head>
<th:block th:include="admin/include :: header('新增水厂水质数据信息（自来水厂/给水厂）')" />
</head>
<body class="white-bg">
<div class="wrapper wrapper-content animated fadeInRight ibox-content">
	<form class="form-horizontal m" id="form-goodWaterHealthInfo-add" th:object="${testTapWaterInfo}">
		<input id="goodWaterHealthInfo_params" name="params[icon]" th:value="${icon}" type="hidden">
		<div class="form-group">
			<label class="col-sm-2 control-label label-left is-required">浊度（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="ntuIn" id="ntuIn" class="form-control" required th:value="*{ntuIn}" 
							type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">NTU</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">浊度 （出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="ntuOut" class="form-control" type="number" th:value="*{ntuOut}"
							min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">NTU</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">色度（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="colourIn" class="form-control" type="number" th:value="*{colourIn}"
							min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">度</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">色度（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="colourOut" class="form-control" type="number" th:value="*{colourOut}" 
							min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">度</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">嗅和味（进水）：</label>
			<div class="col-sm-4">
				<input name="cwIn" class="form-control" maxlength="6" th:value="*{cwIn}">
			</div>
			<label class="col-sm-2 control-label label-left">嗅和味（出水）：</label>
			<div class="col-sm-4">
				<input name="cwOut" class="form-control" maxlength="6" th:value="*{cwOut}">
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">肉眼可见物（进水）：</label>
			<div class="col-sm-4">
				<input name="eyeIn" class="form-control" maxlength="6" th:value="*{eyeIn}">
			</div>
			<label class="col-sm-2 control-label label-left">肉眼可见物（出水）：</label>
			<div class="col-sm-4">
				<input name="eyeOut" class="form-control" maxlength="6" th:value="*{eyeOut}">
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">耗氧量（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="hyIn" class="form-control" th:value="*{hyIn}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">mg/L </span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">耗氧量（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="hyOut" class="form-control" th:value="*{hyOut}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">mg/L</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">氨氮（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="adanIn" class="form-control" th:value="*{adanIn}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">mg/L</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">二氧化氯（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="twoOut" class="form-control" th:value="*{twoOut}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">mg/L</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">细菌总数（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="xjzsIn" class="form-control" th:value="*{xjzsIn}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">CFU/ml</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">细菌总数（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="xjzsOut" class="form-control" th:value="*{xjzsOut}"
						 	type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">CFU/ml</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">总大肠菌群（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="zdjIn" class="form-control" th:value="*{zdjIn}"
							type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">MPN/100ml</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">总大肠菌群（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="zdjOut" class="form-control" th:value="*{zdjOut}"
							type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">MPN/100ml</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">耐热大肠菌群（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="nrjIn" class="form-control" th:value="*{nrjIn}"
							type="number" min="0" max="1000" maxLength="6" oninput="autoformart($(this))">
					<span class="input-group-addon">MPN/100ml</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">耐热大肠菌群（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="nrjOut" class="form-control" th:value="*{nrjOut}"
							type="number" min="0" max="1000" maxLength="4" oninput="autoformart($(this))">
					<span class="input-group-addon">MPN/100ml</span>
				</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 control-label label-left">温度（进水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="tempIn" class="form-control" th:value="*{tempIn}"
							type="number" min="0" max="100" maxLength="3" oninput="autoformart($(this))">
					<span class="input-group-addon">℃</span>
				</div>
			</div>
			<label class="col-sm-2 control-label label-left">温度（出水）：</label>
			<div class="col-sm-4">
				<div class="input-group">
					<input name="tempOut" class="form-control" th:value="*{tempOut}"
							type="number" min="0" max="100" maxLength="3" oninput="autoformart($(this))">
					<span class="input-group-addon">℃</span>
				</div>
			</div>
		</div>
	</form>
</div>
<th:block th:include="admin/include :: common" />
<th:block th:include="admin/include :: form" />
</body>
<script th:inline="javascript">
var form_goodWaterHealthInfo_add_prefix = ctx + "admin/report/testTapWaterInfo";

$("#form-goodWaterHealthInfo-add").validate({
    focusCleanup: true
});

function submitHandler() {
    if ($.validate.form()) {
        $.operate.save(form_goodWaterHealthInfo_add_prefix + "/doAdd", $('#form-goodWaterHealthInfo-add').serialize());
    }
}

function index_goodWaterHealthInfo_submitHandler() {
    if ($.validate.formRequired("form-goodWaterHealthInfo-add")) {
        $.operate.saveModal(form_goodWaterHealthInfo_add_prefix + "/doAdd", $('#form-goodWaterHealthInfo-add').serialize());
    }
}

// input限制
function autoformart(obj, isInt) {
    var val = obj.val();
    if (val == null || val == undefined || val == "") {
        obj.val("");
        return;
    }
    if (isInt && val.indexOf(".") != -1) { // 整数限制
        val = val.slice(0, val.length - 2);
    }
    var maxLength = obj.attr("maxLength");
    var max = obj.attr("max");
    if (val.length > maxLength) {
        obj.val(val.slice(0, maxLength)); // 限制长度
        return;
    }
    if (parseFloat(val) > parseFloat(max)) {
        obj.val(val.slice(0, val.length - 1)); // 限制大小
        return;
    }
    obj.val(parseFloat(val));
}	
</script>
</html>